#include <stdio.h>

int main() {
    int cp, sp;

    printf("Enter cost price: ");
    scanf("%d", &cp);

    printf("Enter selling price: ");
    scanf("%d", &sp);

    if (sp > cp) {
        printf("Profit\n");
    }
    else if (sp < cp) {
        printf("Loss\n");
    }
    else {
        printf("No profit no loss\n");
    }

    return 0;
}
